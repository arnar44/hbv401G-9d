<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="daytours.TripUIController">
   <children>
      <Label fx:id="jtitle" alignment="CENTER" layoutX="179.0" layoutY="30.0" prefHeight="20.0" prefWidth="242.0" text="Titill">
         <font>
            <Font name="Helvetica" size="32.0" />
         </font>
      </Label>
      <TextArea fx:id="jdescription" editable="false" layoutX="199.0" layoutY="92.0" prefHeight="197.0" prefWidth="343.0" wrapText="true" />
      <Label fx:id="jprice" layoutX="39.0" layoutY="145.0" text="Price">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <Label fx:id="jdifficulty" layoutX="39.0" layoutY="167.0" text="Difficulty">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <Label fx:id="jlocation" layoutX="39.0" layoutY="190.0" text="Location">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <Label fx:id="jduration" layoutX="39.0" layoutY="216.0" text="Duration">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <Label layoutX="313.0" layoutY="423.0" text="Name">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <Label layoutX="313.0" layoutY="455.0" text="Email">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <Label layoutX="313.0" layoutY="488.0" text="Quantity">
         <font>
            <Font name="Helvetica" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="jpurchName" layoutX="400.0" layoutY="420.0" promptText="John Johnson" />
      <TextField fx:id="jpurchEmail" layoutX="400.0" layoutY="452.0" promptText="example@example.com" />
      <ComboBox fx:id="jpurchQuantity" layoutX="400.0" layoutY="485.0" prefHeight="27.0" prefWidth="31.0" promptText="0" />
      <Button fx:id="jbookButton" layoutX="477.0" layoutY="539.0" mnemonicParsing="false" onAction="#jbookTrip" prefWidth="90.0" text="Book">
         <font>
            <Font name="Hack Bold" size="16.0" />
         </font>
      </Button>
      <ListView fx:id="jreviewList" layoutX="39.0" layoutY="312.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="jsubmitReview" layoutX="40.0" layoutY="526.0" mnemonicParsing="false" onAction="#jnewReview" style="-fx-background-color: white;" text="Add Review" textAlignment="CENTER">
         <font>
            <Font name="Helvetica" size="13.0" />
         </font>
         <effect>
            <Blend />
         </effect>
      </Button>
   </children>
</AnchorPane>
